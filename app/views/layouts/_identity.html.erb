<% cache 'identity' do %>  <section id="partidegauche" title="<%= t('general.title') %>">    <%= link_to root_path do %><img src="<%= asset_path('PG.png') %>" alt="<%=t 'general.title' %>" class="shadow" width="325" height="108"><% end %>    <a class="adhesion" href="/adhesion" title="<%= t('identity.apply') %>"><img src="<%= asset_path 'adherer.png' %>" alt="<%= t('identity.apply') %>" width="90" height="90"></a>    <span id="dons">      <a class="don" href="/don" title="<%= t('identity.support') %>"><img src="<%= asset_path 'dons.png' %>" alt="<%= t('identity.support') %>" width="90" height="90"></a>      <a class="don" href="/don" title="<%= t('identity.support') %>" style="display:none;"><img src="<%= asset_path 'souscription_demenagement.png' %>" alt="<%= t('identity.support') %>" width="90" height="90"></a>    </span>  </section>  <section id="frontdegauche" title="<%= t('identity.fdg') %>">    <a target="_blank" href="http://www.placeaupeuple2012.fr/"><img src="<%= asset_path 'logoFG210.png' %>" class="shadow" alt="<%= t('identity.fdg') %>" width="210" height="108"></a>  </section><% end %><script>  function toggleDons() {    $('#dons a:first').before($('#dons a:last'));    $('#dons a').hide();    $('#dons a:first').show(100);  };  setInterval('toggleDons()', 3000);</script>