<% cache 'identity' do %><div id="partidegauche" class="logobox">  <span id="logo-home">    <%= link_to root_path do %><img src="<%= asset_path('PG.png') %>" alt="<%=t 'general.title' %>" class="shadow" width="325" height="108"><% end %>    &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  </span>  <a href="/adhesion" title="<%= t('identity.apply') %>">    <img src="<%= asset_path 'adhesion.png' %>" alt="<%= t('identity.apply') %>" width="150" height="97"></a>  &nbsp;&nbsp;&nbsp;&nbsp;&nbsp;&nbsp;  <a href="/don" title="<%= t('identity.support') %>">    <img onmouseover="this.src='<%= asset_path 'rollover.png' %>';"          onmouseout="this.src='<%= asset_path 'rollout.png' %>';"         src="<%= asset_path 'rollout.png' %>"         alt="<%= t('identity.support') %>" width="110" height="97"></a></div><div id="frontdegauche" class="logobox" title="<%= t('identity.fdg') %>">  <a target="_blank" href="http://www.placeaupeuple2012.fr/">    <img src="<%= asset_path 'logoFG300.png' %>" class="shadow" alt="<%= t('identity.fdg') %>" width="210" height="108"></a></div><% end %>