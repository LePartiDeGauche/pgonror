<section id="footer">  <section id="connect">    <section id="connect-content" class="content-bloc highlighted-bloc">      <article class="article">        <header>          <h3 class="category"><%= t('identity.reserved') %>&nbsp;&bull;</h3>        </header>        <br/>        <p class="main-title"><%= link_to t('identity.reserved_link'), t('identity.reserved_web'), :target => "_blank" %></p>        <p>          <%= link_to t('identity.reserved_contact') %>          <%= link_to t('identity.reserved_email'), "mailto:" + t('identity.reserved_email') %>        </p>      </article>    </section>  </section>  <% cache 'network' do %>    <section id="network">      <section id="network-content" class="content-bloc">        <article class="article">          <header>            <h3 class="category"><%= t('identity.follow') %>&nbsp;&bull;</h3>          </header>          <div id="facebook">            <fb:fan profile_id="74597480378"                     stream="0"                     connections="0"                     logobar="0"                     width="252">            </fb:fan>          </div>          <div id="twitter">            <a href="https://twitter.com/LePartideGauche"                class="twitter-follow-button"                data-show-count="true"                data-lang="fr"                data-size="small">Suivre @LePartideGauche</a>            <script>              !function(d,s,id){                var js,fjs=d.getElementsByTagName(s)[0];                if(!d.getElementById(id)){                  js=d.createElement(s);                  js.id=id;                  js.src="//platform.twitter.com/widgets.js";                  fjs.parentNode.insertBefore(js,fjs);                }              }(document,"script","twitter-wjs");            </script>          </div>        </article>      </section>    </section>    <section id="agauche">      <section id="agauche-content" class="content-bloc highlighted-bloc">        <article class="article">          <header>            <h3 class="category"><%= t('identity.agauche') %>&nbsp;&bull;</h3>          </header>          <a href="/agauche" title="<%= t('identity.agauche_title') %>">            <img src="<%= asset_path 'Abonnement.png' %>"                  alt="<%= t('identity.agauche') %>"                  width="210"                 height="98">          </a>        </article>      </section>    </section>    <section id="gavroche">      <section id="gavroche-content" class="content-bloc highlighted-bloc">        <article class="article">          <header>            <h3 class="category"><%= t('identity.gavroche') %>&nbsp;&bull;</h3>          </header>          <br/>          <%= link_to_unless_current t('identity.gavroche_title'), '/gavroche' %>        </article>      </section>    </section>  <% end %></section>