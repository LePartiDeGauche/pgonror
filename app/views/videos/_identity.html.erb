<% cache 'identity-teledegauche' do %>  <section id="partidegauche">    <span title="<%= t 'identity.teledegauche_title' %>">      <%= link_to t('identity.teledegauche_link') do %><img src="<%= asset_path 'logo-tele-de-gauche-large.png' %>" alt="<%= t 'identity.teledegauche_title' %>" width="315" height="108"><% end %>    </span>    <span id="dons">      <a class="don" href="/don" title="<%= t 'identity.support' %>"><img src="<%= asset_path 'dons.png' %>" alt="<%= t 'identity.support' %>" width="90" height="90"></a>      <a class="don" href="/don" title="<%= t 'identity.support' %>" style="display:none;"><img src="<%= asset_path 'souscription_demenagement.png' %>" alt="<%= t 'identity.support' %>" width="90" height="90"></a>    </span>  </section>  <section id="frontdegauche" title="<%= t 'identity.pg_fdg' %>">    <img name="bloc_PG_FDG" src="<%= asset_path 'PG-FDG-small.jpg' %>" alt="<%= t 'general.title' %>" width="107" height="108" id="bloc_PG_FDG" usemap="#m_bloc_PG_FDG" />    <map name="m_bloc_PG_FDG" id="m_bloc_PG_FDG">      <area shape="poly" coords="0,52,107,52,107,108,0,108,0,52" href="<%= t 'identity.fdg_web' %>" target="_blank" title="<%= t 'identity.fdg' %>" alt="<%= t 'identity.fdg' %>" />      <area shape="poly" coords="0,0,107,0,107,38,0,38,0,0" href="<%= root_path %>" title="<%=t 'general.title' %>" alt="<%=t 'general.title' %>" />    </map>  </section><% end %>