<ol>  <% unless @searched_articles.empty? %>    <li><br/></li>    <% first_row = true %>    <% for searched_article in @searched_articles %>      <% controller = searched_article.category_option(:controller) %>      <% controller = :articles if controller.nil? %>      <% action = searched_article.category_option(:action) %>      <% action = :show if action.nil? %>      <% if first_row  %>        <% if searched_article.parent_id.present? %>          <% if @parent_parent_article.present? %>            <br/>            <a id="up-folder">              <%= icon 'previous.png' %>              <%= @parent_parent_article.title_display.html_safe %>            </a>            <script>              $("#up-folder").click(function (event){                $(this).html('<%= t('action.wait') %>');                $("#panel-search-list").load(                  "<%= url_for({:action => :panel_search_page,                                :parent_panel_search => @parent_parent_article.id}) %>"                );              });            </script>            <div class="information"><%= t("action.article.up_explain") %></div>            <br/><br/>          <% end %>        <% end %>        <%= render :partial => 'panel_search_navigator' %>      <% end %>      <li id="article-<%= searched_article.id.to_s %>">        <% if searched_article.category_option?(:document) and searched_article.document_file_name.present? %>          <%= link_to searched_article.title_display, searched_article.document.url %>          <div id="zoom-<%= searched_article.id.to_s %>" class="zoom-article-panel-search" style="display: none;">            <div class="information"><%= t('action.drag_drop') %></div>            <article class="article">              <%= searched_article.status_display_with_style.html_safe %>              <h1><%= searched_article.title_display.html_safe %></h1><br/>              <div id="content-<%= searched_article.id.to_s %>">                <%= link_to t('action.download'), searched_article.document.url, :target => "_blank" %>              </div>              <%= icon('next.png') %><a id="insert-<%= searched_article.id.to_s %>" class="add-link"><%= t('action.insert') %></a>              <script>                $("#insert-<%= searched_article.id.to_s %>").click(function (event){                  tinyMCE.execInstanceCommand('article_content',"mceInsertContent",false,$("#content-<%= searched_article.id.to_s %>").html());                  $("#zoom-<%= searched_article.id.to_s %>").hide();                });              </script>            </article>          </div>        <% elsif searched_article.category_option?(:audio) and searched_article.document_file_name.present? %>          <%= link_to searched_article.title_display, searched_article.document.url %>          <div id="zoom-<%= searched_article.id.to_s %>" class="zoom-article-panel-search" style="display: none;">            <div class="information"><%= t('action.drag_drop') %></div>            <article class="article">              <%= searched_article.status_display_with_style.html_safe %>              <h1><%= searched_article.title_display.html_safe %></h1><br/>              <div id="content-<%= searched_article.id.to_s %>">                <%= link_to t('action.download_audio'), searched_article.document.url, :target => "_blank" %>              </div>              <%= icon('next.png') %><a id="insert-<%= searched_article.id.to_s %>" class="add-link"><%= t('action.insert') %></a>              <script>                $("#insert-<%= searched_article.id.to_s %>").click(function (event){                  tinyMCE.execInstanceCommand('article_content',"mceInsertContent",false,$("#content-<%= searched_article.id.to_s %>").html());                  $("#zoom-<%= searched_article.id.to_s %>").hide();                });              </script>            </article>          </div>        <% elsif searched_article.category_option?(:image) and searched_article.image_file_name.present? %>          <div class="panel-search-item" title="<%= searched_article.title_display.html_safe %>">            <%= image_tag searched_article.image.url(:small) %>          </div>          <div id="zoom-<%= searched_article.id.to_s %>" class="zoom-article-panel-search" style="display: none;">            <div class="information"><%= t('action.drag_drop') %></div>            <article class="article">              <%= searched_article.status_display_with_style.html_safe %>              <h1><%= searched_article.title_display.html_safe %></h1><br/>              <span id="content-inline-<%= searched_article.id.to_s %>">                <%= link_to image_tag(searched_article.image.url(:inline)),                            searched_article.image.url(:original), :target => "_blank" %>                <% if not searched_article.signature.blank? %>                  <br/>                  <small>                    <%= t('activerecord.attributes.article.credit') %>                    <%= searched_article.signature %>                  </small>                <% end %>              </span>              <%= icon('next.png') %><a id="insert-inline-<%= searched_article.id.to_s %>" class="add-link"><%= t('action.insert') %></a>              <br/>              <span id="content-medium-<%= searched_article.id.to_s %>">                <%= link_to image_tag(searched_article.image.url(:medium), :width => Article::MEDIUM_WIDTH),                      searched_article.image.url(:original), :target => "_blank" %>                <% if not searched_article.signature.blank? %>                  <br/>                  <small>                    <%= t('activerecord.attributes.article.credit') %>                    <%= searched_article.signature %>                  </small>                <% end %>              </span>              <%= icon('next.png') %><a id="insert-medium-<%= searched_article.id.to_s %>" class="add-link"><%= t('action.insert') %></a>              <br/>              <span id="content-small-<%= searched_article.id.to_s %>">                <%= link_to image_tag(searched_article.image.url(:small), :width => Article::SMALL_WIDTH),                      searched_article.image.url(:original), :target => "_blank" %>                <% if not searched_article.signature.blank? %>                  <br/>                  <small>                    <%= t('activerecord.attributes.article.credit') %>                    <%= searched_article.signature %>                  </small>                <% end %>              </span>              <%= icon('next.png') %><a id="insert-small-<%= searched_article.id.to_s %>" class="add-link"><%= t('action.insert') %></a>              <br/>              <script>                $("#insert-inline-<%= searched_article.id.to_s %>").click(function (event){                  tinyMCE.execInstanceCommand('article_content',"mceInsertContent",false,$("#content-inline-<%= searched_article.id.to_s %>").html());                  $("#zoom-<%= searched_article.id.to_s %>").hide();                });                $("#insert-medium-<%= searched_article.id.to_s %>").click(function (event){                  tinyMCE.execInstanceCommand('article_content',"mceInsertContent",false,$("#content-medium-<%= searched_article.id.to_s %>").html());                  $("#zoom-<%= searched_article.id.to_s %>").hide();                });                $("#insert-small-<%= searched_article.id.to_s %>").click(function (event){                  tinyMCE.execInstanceCommand('article_content',"mceInsertContent",false,$("#content-small-<%= searched_article.id.to_s %>").html());                  $("#zoom-<%= searched_article.id.to_s %>").hide();                });              </script>            </article>          </div>        <% elsif searched_article.category_option?(:parent) %>          <a id="next-<%= searched_article.id.to_s %>">            <%= icon 'next.png' %>            <%= searched_article.title_display.html_safe %>          </a>          <script>            $("#next-<%= searched_article.id.to_s %>").click(function (event){              $(this).html('<%= t('action.wait') %>');              $("#panel-search-list").load(                "<%= url_for({:action => :panel_search_page,                              :panel_search => session[:panel_search],                              :parent_panel_search => searched_article.id}) %>"              );            });          </script>        <% else %>          <%= searched_article.category_display %>&nbsp;&bull;          <%= link_to searched_article.title_display.html_safe,                      :controller => controller,                      :action => action,                      :uri => searched_article.uri %>          <div id="zoom-<%= searched_article.id.to_s %>" class="zoom-article-panel-search" style="display: none;">            <div class="information"><%= t('action.drag_drop') %></div>            <article class="article">              <%= searched_article.status_display_with_style.html_safe %>              <% unless searched_article.heading.blank? %>                <h4><%= searched_article.heading_display.html_safe %><br/></h4>              <% end %>              <h1><%= searched_article.title_display.html_safe %></h1>              <div class="signature">                <div class="date"><%= l searched_article.published_at, :format => :long_ordinal %></div>&nbsp;                <%= searched_article.signature if searched_article.category_option?(:signature) %>              </div>              <%= link_to searched_article.title_display.html_safe,                          :controller => controller,                          :action => action,                          :uri => searched_article.uri %>            </article>          </div>        <% end %>        <script>          $("#article-<%= searched_article.id.to_s %>").hover(            function (event){              var top = $(this).offset().top-$("#zoom-<%= searched_article.id.to_s %>").height()/4;              var left = 150+$(this).offset().left;              $("#zoom-<%= searched_article.id.to_s %>").css({left:left,top:top}).show(30);            },            function (event){              $("#zoom-<%= searched_article.id.to_s %>").hide();            }          );        </script>      </li>      <% first_row = false %>    <% end %>  <% else %>    <li class="information"><%= t('article.empty') %></li>  <% end %></ol>