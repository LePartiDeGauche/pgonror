<% if @users.present? %>  <ol>  	<% for user in @users %>  		<li>        <article class="article">          <header>            <h3 class="category">  						<%= link_to user.email, user %>  					</h3>            <strong><%= "[" + t('activerecord.attributes.user.administrator') + "]" if user.administrator? %></strong>            <%= "[" + t('activerecord.attributes.user.publisher') + "]" if user.publisher? %>            <%= user.access_level_display if user.access_level.present? %>  				</header>  			</article>  		</li>  	<% end %>  </ol><% else %>  <%= t('user.empty') %><% end %>